{
    "name": "nzta/gallery",
    "description": "A module for creating a gallery of images with captions",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/NZTA/gallery",
    "keywords": [
        "silverstripe",
        "image",
        "gallery",
        "cms"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/NZTA/gallery/issues"
    },
    "require": {
        "silverstripe/cms": "^4 || ^5",
        "symbiote/silverstripe-gridfieldextensions": "^3 || ^4"
    },
    "require-dev": {
        "slevomat/coding-standard": "^8",
        "phpunit/phpunit": "^9"
    },
    "extra": {
        "expose": [
            "css",
            "js",
            "icons"
        ]
    },
    "autoload": {
        "psr-4": {
            "NZTA\\Gallery\\": "src",
            "NZTA\\Gallery\\Test\\": "tests"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
